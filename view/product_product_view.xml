<?xml version="1.0" encoding="utf-8">
	<<openerp>
		<data>
			<record id="view_importacion_pedimento_form" model="ir.ui.view">
				<<field name="name">importacion.form</field>
				<field name="model">product.product</field>
				<<field name="type">form</field>
				<field name="inherit_id">base.product_normal_form</field>
				<field name="arch" type="xml">
					<xpath expr="//notebook/page[@string=Procurements]/group/group/field[@name='standard_price']" position="after">
						<field name="is_import"/>
					</xpath>
			</record>
		</data>
	</openerp>

